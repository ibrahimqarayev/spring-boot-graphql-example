type Query{
    gwtAllUsers:[UserDto]
}

type Mutation{
    createUser(userRequest:UserRequest!):UserDto
    updateUser(userRequest:UserRequest!):UserDto
    deleteUser(id:ID!):Boolean
}

type UserDto{
    id:ID
    username:String
    email:String
    role:Role
}

enum Role {
    USER
    ADMIN
}

input UserRequest{
    id:ID
    username:String
    email:String
    role:Role
}